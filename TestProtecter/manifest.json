{
	"content_scripts": [ {
      "js": [ "content.js" ],
      "matches": [ "file:///*", "http://*/*", "https://*/*" ]
   } ],
   "browser_action":{
      "default_icon" : "icon16.png",
      "default_title":"finprotector",
      "default_popup":"popup.html"
   },
   "description": "To protect user from being fingerprinted",
   "icons": { "16": "icon16.png", "32": "icon32.png", "48": "icon48.png", "128": "icon128.png" },
   "manifest_version": 2,
   "name": "Finprotector 1.1",
   "short_name": "Finprotector",
   "permissions": [ "nativeMessaging" ,"management", "storage", "tabs", "<all_urls>", "unlimitedStorage"],
   "version": "0.0.1",
   "offline_enabled": true
}


